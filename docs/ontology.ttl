:ALEstDeExterne rdf:type owl:Class ;
                rdfs:subClassOf :ALEstDe ,
                                :RelationSpatialeDisjointe ;
                :atAbstractClass "false"^^xsd:boolean ;
                rdfs:comment "La cible se situe globalement à l’est et est disjointe du site."@fr ;
                rdfs:label "À l'est de externe"@fr .


###  http://www.purl.org/orl#ALExtremiteDe
:ALExtremiteDe rdf:type owl:Class ;
               rdfs:subClassOf :RelationSpatialeDeContact ;
               <http://purl.org/vocab/vann/example> "Il est à l'extrémité ouest d'une espèce de terrasse en béton."@fr ;
               :atAbstractClass "false"^^xsd:boolean ;
               rdfs:comment "Le site est de forme allongée (il a alors deux extrémités), ou possède une ou plusieurs parties saillantes allongées (il peut alors avoir plusieurs extrémités).  La cible est située au niveau (ProcheDe) l'une de ces extrémités. Elle peut être située à l'intérieur du site, ou non."@fr ;
               rdfs:label "À l'extrémité de"@fr ;
               rdfs:seeAlso <http://www.ontospace.uni-bremen.de/ontology/stable/GUM-3-space.owl#Peripheral> .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :ALExtremiteDe ;
   owl:annotatedProperty <http://purl.org/vocab/vann/example> ;
   owl:annotatedTarget "Il est à l'extrémité ouest d'une espèce de terrasse en béton."@fr ;
   <http://purl.org/dc/terms/source> "Malaise bastille 4-2"
 ] .


