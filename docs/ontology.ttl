[ rdf:type owl:Axiom ;
   owl:annotatedSource :AuNordDeExterne ;
   owl:annotatedProperty <http://purl.org/vocab/vann/example> ;
   owl:annotatedTarget "non, côté Nord (du Pas de la Ville), pardon."@fr ;
