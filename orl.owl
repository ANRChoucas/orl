        <orl:atAbstractClass rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</orl:atAbstractClass>
        <rdfs:comment xml:lang="fr">La cible est au dessus du site et sur un chemin menant au site</rdfs:comment>
        <rdfs:label xml:lang="fr">Au dessus d&apos;un jalon sur un chemin</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://www.ontospace.uni-bremen.de/ontology/stable/GUM-3-space.owl#AboveProjectionExternal"/>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.purl.org/choucas.ign.fr/orl#AuDessusJalonSurItineraire"/>
        <owl:annotatedProperty rdf:resource="http://purl.org/vocab/vann/example"/>
        <owl:annotatedTarget xml:lang="fr">...sur le sentier qui mène à la cascade de l’oursière, haut dessus de la cascade de l’oursière</owl:annotatedTarget>
        <dcterms:source>Cascade de l&apos;ourisère 1-3</dcterms:source>
    </owl:Axiom>
    


    <!-- http://www.purl.org/choucas.ign.fr/orl#AuDessusProche -->

    <owl:Class rdf:about="http://www.purl.org/choucas.ign.fr/orl#AuDessusProche">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/choucas.ign.fr/orl#RelationSpatialeProximité"/>
        <rdfs:subClassOf rdf:resource="http://www.purl.org/choucas.ign.fr/orl#RelationSpatialeVerticale"/>
        <vann:example xml:lang="fr">juste au-dessus de Bernin</vann:example>
        <orl:atAbstractClass rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</orl:atAbstractClass>
        <rdfs:comment xml:lang="fr">La cible est proche et a une altitude supérieure à celle du site.</rdfs:comment>
        <rdfs:label xml:lang="fr">Au dessus et proche de</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://www.ontospace.uni-bremen.de/ontology/stable/GUM-3-space.owl#AboveProjectionExternal"/>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.purl.org/choucas.ign.fr/orl#AuDessusProche"/>
        <owl:annotatedProperty rdf:resource="http://purl.org/vocab/vann/example"/>
        <owl:annotatedTarget xml:lang="fr">juste au-dessus de Bernin</owl:annotatedTarget>
        <dcterms:source>Saint Pancrasse 4-1</dcterms:source>
    </owl:Axiom>
    


    <!-- http://www.purl.org/choucas.ign.fr/orl#AuNordDe -->

    <owl:Class rdf:about="http://www.purl.org/choucas.ign.fr/orl#AuNordDe">
